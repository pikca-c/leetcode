      if nums[0] > nums[1]:
            for i in range(len(nums)-1):
                if nums[i] >= nums[i+1]:
                    continue
                else: return False
            return True
        elif nums[0] < nums[1]:
            for i in range(len(nums)-1):
                if nums[i] <= nums[i+1]:
                    continue
                else: return False
            return True
        elif nums[0]==nums[1]:
            return True
